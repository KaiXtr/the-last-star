        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Opções
draw_set_colour(c_white)
draw_set_font(fonte)
draw_surface(paint,0,0)


//Palavras
if menu&lt;3{
if global.idioma=1{draw_text(120,40,"select your destiny")}
if global.idioma=2{draw_text(120,40,"escolha seu destino")}
if global.idioma=3{draw_text(120,40,"elegir su destino")}
}


//Camera
if rota=false and menu=1 and stop=false{
if mouse_check_button_pressed(mb_left){camera=true}}
if mouse_check_button_released(mb_left){camera=false}


//Traçar Rota
if rota=true
{if mouse_check_button_pressed(mb_left)
{pointnumbers=instance_number(Obj_point)
point[pointnumbers]=instance_create(mouse_x,mouse_y,Obj_point)}

with(Obj_point)if pointnumbers=Obj_stageselect.pointnumbers
{with(Obj_stageselect)move_towards_point(point[pointnumbers].x,point[pointnumbers].y,0)
global.gamedirection[real(pointnumbers-1)]=direction}

point[real(pointnumbers+1)].x=mouse_x
point[real(pointnumbers+1)].y=mouse_y

for(pointnumbers=1;pointnumbers&lt;array_length_1d(point); pointnumbers++)
draw_line(point[pointnumbers].x,point[pointnumbers].y,point[pointnumbers+1].x,point[pointnumbers+1].y);
}





//Gasolina
if menu&lt;3{
draw_sprite_ext(Spr_fuelbarra,0,40,450,88,1,0,c_red,1)
draw_sprite_ext(Spr_fuelborda,1,40,450,global.fuel/100,1,0,c_white,1)
draw_sprite(Spr_fuelborda,0,40,450)}


//Pronto?
if menu&lt;3{
if stop=true{

draw_text(200,200,"está tudo certo?")

if go=true{draw_set_colour(c_red)}
else
{draw_set_colour(c_white)}
draw_text(200,220,"sim")

if go=false{draw_set_colour(c_red)}
else
{draw_set_colour(c_white)}
draw_text(240,220,"não")
}}


//Missões
if menu=2{
draw_set_font(fontinha)
tile_layer_show(-97)
draw_sprite(Spr_botao,0,200,200)
draw_text(210,205,"traçar rota")
draw_sprite(Spr_botao,0,300,200)
draw_text(312,205,"cancelar")
}


//Select
if mouse_check_button_released(mb_left){
if mouse_x&gt;200 and mouse_x&lt;283{
if mouse_y&gt;200 and mouse_y&lt;222{
audio_play_sound(Sou_escudo,10,false)}}
if mouse_x&gt;300 and mouse_x&lt;383{
if mouse_y&gt;200 and mouse_y&lt;222{
menu=1}}
}


//Zoom
if mouse_wheel_up(){
if image_xscale&lt;1.5{
image_xscale+=0.1;image_yscale+=0.1
with(Obj_planetas){image_xscale+=0.1;image_yscale+=0.1}
with(Obj_rastro){image_xscale+=0.1;image_yscale+=0.1}
background_xscale[0]+=0.1;background_yscale[0]+=0.1}
}

if mouse_wheel_down(){
if image_xscale&gt;0.3{
image_xscale-=0.1;image_yscale-=0.1
with(Obj_planetas){image_xscale-=0.1;image_yscale-=0.1}
with(Obj_rastro){image_xscale-=0.1;image_yscale-=0.1}
background_xscale[0]-=0.1;background_yscale[0]-=0.1}
}



//Loja
if menu&gt;2{
tile_layer_show(-98)
draw_sprite(Spr_estrelas,0,60,50)
draw_text(90,50,global.estrelas)

draw_text(60,100,"armas")
draw_text(60,120,"escudos")
draw_text(60,140,"bombas")
draw_text(60,160,"melhorias")
draw_text(60,180,"customizar")
draw_text(60,200,"sair")

if stop=false{
if menu=3{draw_sprite(Spr_select,0,20,100)}
if menu=4{draw_sprite(Spr_select,0,20,120)}
if menu=5{draw_sprite(Spr_select,0,20,140)}
if menu=6{draw_sprite(Spr_select,0,20,160)}
if menu=7{draw_sprite(Spr_select,0,20,180)}
if menu=8{draw_sprite(Spr_select,0,20,200)}
}

if stop=true{
if menu=7
{if opcao=1{draw_sprite(Spr_select,0,250,60)}
if opcao=2{draw_sprite(Spr_select,0,250,80)}
if opcao=3{draw_sprite(Spr_select,0,250,100)}
if opcao=4{draw_sprite(Spr_select,0,250,120)}
if opcao=5{draw_sprite(Spr_select,0,250,140)}
if opcao=6{draw_sprite(Spr_select,0,250,160)}
if opcao=7{draw_sprite(Spr_select,0,250,180)}}
}

if menu=3{
draw_sprite(Spr_armas,0,310,85)
draw_sprite(Spr_armas,0,410,85)
draw_sprite(Spr_armas,0,510,85)
draw_sprite(Spr_armas,0,310,185)
draw_sprite(Spr_armas,0,410,185)
draw_sprite(Spr_armas,0,510,185)
draw_sprite(Spr_armas,0,310,285)
draw_sprite(Spr_armas,0,410,285)
draw_sprite(Spr_armas,0,510,285)
}

if menu=7{
draw_text(280,60,"canhão")
draw_text(280,80,"cabine")
draw_text(280,100,"base")
draw_text(280,120,"asas")
draw_text(280,140,"spoilers")
draw_text(280,160,"propulsor")
draw_text(280,180,"sair")

draw_sprite_ext(Spr_canhao,global.canhao,400,120-19,1,1,image_angle,global.canhaocor,1)
draw_sprite_ext(Spr_propulsor,global.propulsor,400,120+15,1,1,image_angle,c_white,1)
draw_sprite_ext(Spr_asas,global.asa,400,120+30,-1,1,image_angle,global.asacor,1)
draw_sprite_ext(Spr_asas,global.asa,400,120+30,1,1,image_angle,global.asacor,1)
draw_sprite_ext(Spr_base,global.base,400,120,1,1,image_angle,global.basecor,1)
draw_sprite_ext(Spr_vidro,global.vidro,400,120-7,1,1,image_angle,global.vidrocor,1)
draw_sprite_ext(Spr_spoilers,global.spoiler,400+4,120,1,1,image_angle,global.spoilercor,1)
}


draw_line(60,400,line1,420)
draw_line(60,400,line2,420)
}
</string>
